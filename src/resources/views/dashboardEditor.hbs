<link rel="stylesheet" href="/css/dashboard.css">
<form method="post" action="/user/dashboard/editor" class="search-container ">
    <div class="search-result-container line-heigh-15 flex-style flex-column">
        <div class="padding-16">
            <h2 class="margin-bot-24">Đơn đăng ký nhãn hiệu "{{outputData.mo_ta_nhan_hieu}}" số
                {{outputData.so_don}}
                của {{outputData.chu_don}}</h2>
            <div class="grid-style grid-col-1-2-4">
                <div class="grid-item bold">Mẫu nhãn</div>
                <div class="grid-item"><img class="trademark-logo" src="{{outputData.mau_nhan}}" alt=""></div>
                <div class="grid-item grid-style margin-top-24">
                    <div class="grid-style grid-col-4 margin-top-16">
                        <div class="grid-item bold">Trạng thái</div>
                        <textarea name="tinh_trang" class="grid-item">{{outputData.tinh_trang}}</textarea>
                    </div>
                    <div class="grid-style grid-col-4 margin-top-16">
                        <div class="grid-item bold">Số đơn</div>
                        <textarea class="grid-item" name="so_don" readonly>{{outputData.so_don}}</textarea>
                        <div class="grid-item bold">Số đơn gốc</div>
                        <textarea name="so_don_goc" class="grid-item">(Số đơn gốc)</textarea>
                    </div>
                    <div class="grid-style grid-col-1-3 margin-top-16">
                        <div class="grid-item bold">Ngày nộp</div>
                        <textarea name="ngay_nop_don" class="grid-item">{{outputData.ngay_nop_don}}</textarea>
                    </div>
                    <div class="grid-style grid-col-4 margin-top-16">
                        <div class="grid-item bold">Tháng công bố</div>
                        <textarea name="" class="grid-item select-box">(Tháng công bố)</textarea>
                    </div>
                    <div class="grid-style grid-col-1-3 margin-top-16">
                        <div class="grid-item bold">Chủ đơn/Chủ bằng</div>
                        <div class="grid-item">
                            <textarea name="chu_don" class="select-box">{{outputData.chu_don}}</textarea>
                            <br>
                            <textarea name="dia_chi_chu_don"
                                class="select-box">{{outputData.dia_chi_chu_don}}</textarea>
                        </div>
                    </div>
                </div>
            </div>
            <div class="grid-style grid-col-4 margin-top-16">
                <div class="grid-item bold">Mô tả nhãn hiệu</div>
                <textarea name="mo_ta_nhan_hieu" class="grid-item">{{outputData.mo_ta_nhan_hieu}}</textarea>
                <div class="grid-item bold">Màu sắc</div>
                <textarea name="mau_sac_nhan_hieu" class="grid-item">{{outputData.mau_sac}}</textarea>
            </div>
            <div class="grid-style grid-col-1-3 margin-top-16">
                <div class="grid-item bold">Danh mục sản phẩm/dịch vụ</div>
                <div class="grid-item align-justify">
                    {{{outputData.nhom_san_pham}}}
                </div>
            </div>
            <div class="grid-style grid-col-1-3 margin-top-16">
                <div class="grid-item bold">Phân loại hình</div>
                <textarea name="phan_loai_hinh" class="grid-item">{{outputData.phan_loai_hinh}}</textarea>
            </div>
            <div class="grid-style grid-col-1-3 margin-top-16">
                <div class="grid-item bold">Đại diện SHCN</div>
                <div class="grid-item">
                    {{{outputData.dai_dien_shcn}}}
                </div>
            </div>
            <hr class="margin-top-24 margin-bot-24">
            <h3 class="margin-bot-24 small-title">Chi tiết cấp bằng</h3>
            <div class="grid-style grid-col-5-1">
                <div>
                    <div class="grid-item grid-style grid-col-6">
                        <div class="grid-item bold">Số bằng</div>
                        <textarea name="so_bang" class="grid-item"></textarea>
                        <div class="grid-item bold">Ngày cấp</div>
                        <textarea name="" class="grid-item" ></textarea>
                        <div class="grid-item bold">Thời gian</div>
                        <textarea name="" class="grid-item" ></textarea>
                    </div>
                    <div class="grid-item grid-style margin-top-16 grid-col-4">
                        <div class="grid-item bold">Mô tả nhãn hiệu</div>
                        <textarea name="" class="grid-item">{{outputData.mo_ta_nhan_hieu_cb}}</textarea>
                        <div class="grid-item bold">Màu sắc</div>
                        <textarea name=""
                            class="grid-item">{{outputData.mau_sac_cb}}</textarea>
                    </div>
                    <div class="grid-item grid-style grid-col-1-3 margin-top-16">
                        <div class="grid-item bold">Danh mục sản phẩm/Dịch vụ</div>
                        <div class="grid-item align-justify">{{{outputData.nhom_san_pham_cb}}}</div>
                    </div>
                    <div class="grid-item grid-style grid-col-1-3 margin-top-16">
                        <div class="grid-item bold">Phân loại hình</div>
                        <textarea name=""
                            class="grid-item">{{outputData.phan_loai_hinh_cb}}</textarea>
                    </div>
                    <div class="grid-item grid-style grid-col-1-3 margin-top-16">
                        <div class="grid-item bold">Tuyên bố bảo hộ/yếu tố loại trừ</div>
                        <textarea name="yeu_to_loai_tru" class="grid-item">{{outputData.yeu_to_loai_tru}}</textarea>
                    </div>
                    <div class="grid-item grid-style grid-col-1-3 margin-top-16">
                        <div class="grid-item bold">Đại diện SHCN</div>
                        <textarea name="" class="grid-item">na</textarea>
                    </div>
                </div>
            </div>
            <hr class="margin-top-24 margin-bot-24">
            <h3 class="margin-bot-24 margin-top-24 small-title">Tiến trình xử lý</h3>
            <div class="grid-style grid-col-3">
                <div class="grid-item small-title">Tiến trình</div>
                <div class="grid-item small-title">Ngày tiến trình</div>
                <div class="grid-item small-title">Tài liệu</div>
            </div>
            {{{outputData.tttd_html}}}
        </div>
    </div>
    <div class="search-result-container line-heigh-15 flex-style margin-bot-14vh flex-column">
        <div class="padding-16 flex-grow-1 flex-style flex-column">
            <div class="list-report-container">
                <p class="small-title">Đơn chỉnh sửa</p>
                <div class="user-search-container">
                    <button type="submit" class="button-style">Trình Duyệt</button>
                    {{{reportContent}}}
                </div>
            </div>
        </div>
    </div>
</form>